
config system
	option hostname 'OpenWrt'
	option ttylogin '0'
	option log_size '64'
	option urandom_seed '0'
	option timezone 'CST-8'
	option zonename 'Asia/Shanghai'
	option hostname 'Openwrt_x86'

config timeserver 'ntp'
	option enabled '1'
	option enable_server '0'
	list server 'ntp1.aliyun.com'
	list server 'time1.cloud.tencent.com'
	list server 'time.ustc.edu.cn'
	list server 'cn.pool.ntp.org'

config led 'led_wan'
	option name 'WAN'
	option sysfs 'nanopi-r2s:green:wan'
	option default '0'
	option trigger 'none'

config led 'led_lan'
	option name 'LAN'
	option sysfs 'nanopi-r2s:green:lan'
	option default '0'
	option trigger 'none'

config led
	option default '0'
	option name 'sys'
	option sysfs 'nanopi-r2s:red:sys'
	option trigger 'none'

